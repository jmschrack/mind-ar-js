{
  "name": "mind-ar",
  "version": "1.1.6",
  "description": "web augmented reality framework",
  "main": "index.js",
  "scripts": {
    "watch": "webpack build --watch --config webpack.config.dev.cjs",
    "build": "vite build --config vite.config.prod.js && webpack build --config webpack.config.prod.cjs",
    "build-dev": "vite build --config vite.config.dev.js && webpack build --config webpack.config.dev.cjs"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/hiukim/mind-ar-js"
  },
  "keywords": [
    "web AR",
    "aframe",
    "web augmented reality",
    "image tracking",
    "face tracking",
    "tensorflowjs",
    "mediapipe"
  ],
  "author": "hiukim",
  "license": "MIT",
  "dependencies": {
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@msgpack/msgpack": "^1.12.2",
    "@tensorflow/tfjs": "^3.21.0",
    "canvas": "^2.10.2",
    "ml-matrix": "^6.6.0",
    "svd-js": "^1.1.1",
    "tinyqueue": "^2.0.3"
  },
  "peerDependencies": {
    "three": ">=0.136.0"
  },
  "devDependencies": {
    "css-loader": "^5.2.7",
    "html-loader": "^2.1.2",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "vite": "^3.2.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
